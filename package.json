  "scripts": {
    "clean": "rimraf ./docs",
    "start": "npm-run-all -s clean parcel_start",
    "parcel_start": "parcel 'src/*.html' --dist-dir docs",
    "build": "npm-run-all -s clean parcel_build",
    "parcel_build": "parcel build 'src/*.html' --dist-dir docs --public-url ./"
  },
  "repository": {
    "type": "git",
